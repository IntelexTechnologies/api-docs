      <span class="p">{</span> 
          <span class="na">EquipmentAttributeId</span><span class="p">:</span> <span class="s1">'number'</span><span class="p">,</span>
          <span class="na">EquipmentId</span><span class="p">:</span> <span class="s1">'number'</span><span class="p">,</span>
          <span class="na">AttributeTypeId</span><span class="p">:</span> <span class="s1">'number'</span><span class="p">,</span>
