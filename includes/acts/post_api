  <span class="na">headers</span><span class="p">:</span> <span class="p">{</span> <span class="s1">'content-type'</span><span class="p">:</span> <span class="s1">'application/json'</span> <span class="p">},</span>
  <span class="na">body</span><span class="p">:</span>
      <span class="p">{</span>
        <span class="s2">"workflowId"</span><span class="p">:</span> <span class="s2">"number"</span><span class="p">,</span>
        <span class="s2">"workflowTypeId"</span><span class="p">:</span> <span class="s2">"number"</span><span class="p">,</span>
        <span class="s2">"workflowDate"</span><span class="p">:</span> <span class="s2">"2024-01-03T08:16:24.155Z"</span><span class="p">,</span>
        <span class="s2">"dataLockTypeId"</span><span class="p">:</span> <span class="s2">"number"</span><span class="p">,</span>
